{
	"name": "vk-block",
	"version": "1.0.0",
	"description": "",
	"main": "gulpfile.js",
	"scripts": {
		"start": "npm run build && npx wp-env start && open http://localhost:8888",
		"stop": "wp-env stop",
		"destroy": "wp-env destroy",
		"watch": "npx gulp watch",
		"update": "npx ncu -u -x 'sass react react-dom'",
		"dist": "composer install --no-dev && npx gulp dist",
		"prebuild": "npx rimraf ./inc/vk-blocks/build/*",
		"build:dev": "composer install --no-dev && npx gulp text-domain && wp-scripts build --config webpack.config.js --mode=development && npx gulp build:dev:pro",
		"build": "composer install --no-dev && npx gulp text-domain && wp-scripts build --config webpack.config.js && npx gulp build:pro && npm run translate && npm run build:blocks && npm run build:extensions:sass",
		"build:free": "composer install --no-dev && npx gulp text-domain && wp-scripts build --config webpack.config.js && npx gulp build:free && npm run build:blocks:free",
		"build:pot": "wp i18n make-pot --merge=inc/vk-blocks/languages/vk-blocks-js.pot --skip-js . inc/vk-blocks/languages/vk-blocks.pot",
		"translate": "npm run build:pot && po2json ./inc/vk-blocks/languages/vk-blocks-ja.po ./inc/vk-blocks/languages/vk-blocks-ja-vk-blocks-build-js.json -f jed",
		"gulp": "npx gulp",
		"lint": "wp-scripts format src/ && wp-scripts lint-js src/ --fix",
		"lint:php": "composer install && composer format && composer phpcs",
		"fixtures:clean": "rimraf \"test/e2e-tests/fixtures/blocks/*.+(json|serialized.html)\"",
		"fixtures:generate": "cross-env GENERATE_MISSING_FIXTURES=y wp-scripts test-unit-js --config test/jest.config.js test/integration/full-content/full-content.test.js",
		"fixtures:regenerate": "npm run build && npm run fixtures:clean && npm run fixtures:generate",
		"test-unit": "npm run test-unit:clean && npm run fixtures:regenerate",
		"test-unit:withcache": "npm run fixtures:regenerate",
		"test-unit:clean": "wp-scripts test-unit-js --clearCache",
		"phpunit": "composer install && npx wp-env run phpunit \"phpunit -c /var/www/html/wp-content/plugins/vk-blocks-pro/phpunit.xml.dist --verbose\"",
		"cli": "wp-env run cli",
		"show:savedVersion": "bash -c 'npm run cli wp post meta get $0 _vkb_saved_block_version'",
		"update:savedVersion": "bash -c 'npm run cli wp post meta update $0 _vkb_saved_block_version $1'",
		"build:js": "wp-scripts build src/blocks/$npm_config_block/index.js --output-path=build/$npm_config_block --config webpack.config.individual.js",
		"build:js:dev": "wp-scripts build src/blocks/$npm_config_block/index.js --output-path=build/$npm_config_block --mode=development --config webpack.config.individual.js",
		"build:sass": "npx sass src/blocks/$npm_config_block/style.scss build/$npm_config_block/style.css --style=compressed --no-source-map",
		"build:postcss": "npx postcss build/$npm_config_block/style.css --use=autoprefixer --no-map --replace",
		"build:css": "run-s build:sass build:postcss --block=$npm_config_block",
		"build:jspro": "wp-scripts build src/blocks/_pro/$npm_config_block/index.js --output-path=build/_pro/$npm_config_block --config webpack.config.individual.js",
		"build:jspro:dev": "wp-scripts build src/blocks/_pro/$npm_config_block/index.js --output-path=build/_pro/$npm_config_block --mode=development --config webpack.config.individual.js",
		"build:sasspro": "npx sass src/blocks/_pro/$npm_config_block/style.scss:build/_pro/$npm_config_block/style.css --style=compressed --no-source-map",
		"build:postcsspro": "npx postcss build/_pro/$npm_config_block/style.css --use=autoprefixer --no-map --replace",
		"build:csspro": "run-s build:sasspro build:postcsspro --block=$npm_config_block",
		"build:block": "run-s build:{js,css} --block=$npm_config_block",
		"build:block:dev": "run-s build:{js:dev,css} --block=$npm_config_block",
		"build:blockpro": "run-s build:{jspro,csspro} --block=$npm_config_block",
		"build:blockpro:dev": "run-s build:{jspro:dev,csspro} --block=$npm_config_block",
		"build:blocks": "node build.mjs",
		"build:blocks:dev": "node build.mjs dev",
		"build:extensions:sass": "run-s build:extensions/*",
		"build:extensions/hidden-extension": "sass src/extensions/common/hidden-extension/style.scss build/extensions/common/hidden-extension/style.css --style=compressed --no-source-map",
		"build:extensions/responsive-br": "sass src/extensions/common/responsive-br/style.scss build/extensions/common/responsive-br/style.css --style=compressed --no-source-map",
		"build:blocks:free": "node build.mjs free",
		"reinstall": "rm -rf node_modules/ && npm install && npm run build",
		"prepare": "husky install"
	},
	"author": "",
	"license": "MIT",
	"browserslist": [
		"last 2 version",
		"> 5%"
	],
	"devDependencies": {
		"@emotion/babel-plugin": "^11.3.0",
		"@wordpress/babel-plugin-makepot": "^4.2.0",
		"@wordpress/block-editor": "^8.0.5",
		"@wordpress/block-library": "^6.0.9",
		"@wordpress/block-serialization-default-parser": "^4.2.3",
		"@wordpress/blocks": "^11.1.4",
		"@wordpress/components": "^19.0.5",
		"@wordpress/data": "^6.1.4",
		"@wordpress/edit-post": "^5.0.11",
		"@wordpress/element": "^4.0.4",
		"@wordpress/env": "^4.1.3",
		"@wordpress/hooks": "^3.2.2",
		"@wordpress/i18n": "^4.2.4",
		"@wordpress/icons": "^6.1.1",
		"@wordpress/list-reusable-blocks": "^3.0.9",
		"@wordpress/plugins": "^4.0.6",
		"@wordpress/scripts": "^19.2.2",
		"@wordpress/viewport": "^4.0.6",
		"@wp-g2/create-styles": "0.0.164",
		"autoprefixer": "^10.4.0",
		"babel-loader": "^8.2.3",
		"babel-plugin-inline-json-import": "^0.3.2",
		"classnames": "^2.3.1",
		"compare-versions": "^4.1.1",
		"cross-env": "^7.0.3",
		"eslint-plugin-prettier": "^4.0.0",
		"gettext-parser": "4.2.0",
		"gettext-to-messageformat": "0.4.0",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-concat": "^2.6.1",
		"gulp-notify": "^4.0.0",
		"gulp-plumber": "^1.2.1",
		"gulp-rename": "^2.0.0",
		"gulp-replace": "^1.1.3",
		"gulp-sass": "^5.0.0",
		"gulp-uglify": "^3.0.2",
		"husky": "^7.0.4",
		"jest-transform-stub": "^2.0.0",
		"lodash": "^4.17.21",
		"memize": "^1.1.0",
		"nodegit": "^0.27.0",
		"npm-check-updates": "^12.0.2",
		"npm-run-all": "^4.1.5",
		"po2json": "^1.0.0-beta-3",
		"postcss-cli": "^9.0.2",
		"prettier": "^2.4.1",
		"react": "16.14.0",
		"react-dom": "16.14.0",
		"react-html-parser": "^2.0.2",
		"react-with-direction": "^1.4.0",
		"rimraf": "^3.0.2",
		"sass": "1.32.12"
	}
}
